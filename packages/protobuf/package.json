{
  "name": "@dritelabs/project-starter-protobuf",
  "version": "0.0.0",
  "description": "My awesome typescript library",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": "./dist/index.mjs",
    "./dist/server": "./dist/server.mjs",
    "./dist/client": "./dist/client.mjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/*.ts src/**/**/*.ts --format cjs,esm --clean",
    "build:dev": "pnpm build --dts-resolve",
    "generate": "proto-loader-gen-types --longs=String --enums=String --defaults --oneofs --arrays --grpcLib=@grpc/grpc-js --outDir=src/protobuf/ proto/*.proto",
    "fix": "gts fix",
    "lint": "gts lint",
    "test": "vitest run",
    "prepublishOnly": "pnpm build",
    "prepare": "pnpm build",
    "pretest": "pnpm build",
    "posttest": "pnpm lint"
  },
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.7.1",
    "@grpc/proto-loader": "^0.7.3"
  },
  "devDependencies": {
    "@types/node": "18.7.23",
    "eslint": "^8.24.0",
    "gts": "^3.1.1",
    "tsup": "6.2.3",
    "typescript": "4.8.4",
    "vitest": "0.23.4"
  }
}
